    "def low_rank_approx(U, sigma, Vt, k):\n",
    "    '''\n",
    "    I'll assume that sigma is full\n",
    "    '''\n",
    "    res = np.zeros((U.shape[0], Vt.shape[1]))\n",
    "    for i in range(k):\n",
    "        res += np.outer(U[:,i], Vt[i,:])*sigma[i,i]\n",
    "    return res"
   ]
